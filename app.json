{
  "expo": {
    "name": "Patient Management",
    "slug": "oncarecancer",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "https://www.oncarecancer.com/img/featured/2025-10-22%2C10%3A26%3A10.122Z-logo%20%283%29.png",
    "notification": {
      "icon": "https://www.oncarecancer.com/img/featured/2025-10-22%2C10%3A26%3A10.122Z-logo%20%283%29.png",
      "color": "#20606B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oncare.patientmanagementapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fcf640f1-4ef7-4fdf-abe0-cdd3e913ccb9"
      }
    },
    "android": {
      "package": "com.oncare.patientmanagementapp",
      "googleServicesFile": "./google-services.json"
    },
    "owner": "oncarecancer-app"
  }
}
