{
  "expo": {
    "name": "Patient Management",
    "slug": "oncarecancer",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#20606B"
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#20606B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.oncare.patientmanagementapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fcf640f1-4ef7-4fdf-abe0-cdd3e913ccb9"
      }
    },
    "android": {
      "versionCode": 3,
      "package": "com.oncare.patientmanagementapp",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#20606B"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "DOWNLOAD_WITHOUT_NOTIFICATION"
      ]
    },
    "owner": "oncarecancer-app",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/fcf640f1-4ef7-4fdf-abe0-cdd3e913ccb9"
    }
  }
}
