{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "EXPO_NO_TELEMETRY=1 expo start",
    "build:web": "expo export --platform web",
    "lint": "expo lint",
    "typecheck": "tsc --noEmit",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "local-build": "sudo rm -rf android ios && sudo rm -rf .expo && sudo rm -rf ~/.gradle/caches/ && sudo rm -rf android/.gradle/  && sudo rm -rf .expo .expo-shared && sudo rm -rf ~/.gradle/caches/ && sudo rm -rf ~/.gradle/daemon/ && sudo rm -rf node_modules/.cache && sudo rm -rf ~/.eas/build-cache/ && sudo npm cache clean --force && sudo rm -rf /tmp/metro-* && echo 'going to prebuild' && npx expo prebuild --clean && node scripts/locally-fix-gradle.js && cd android && echo 'gradelw clean now' && ./gradlew clean && cd ../ && echo 'going to build' && NODE_OPTIONS=--max_old_space_size=8192 eas build --platform android --local"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@lucide/lab": "^0.1.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-firebase/app": "^23.4.1",
    "@react-native-firebase/messaging": "^23.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.2",
    "expo": "^54.0.10",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.8",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.16",
    "expo-device": "^8.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-notifications": "^0.32.12",
    "expo-router": "~6.0.8",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.12",
    "expo-web-browser": "~15.0.7",
    "lucide-react-native": "^0.544.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.1.10",
    "react-native-worklets": "^0.6.1",
    "typescript": "~5.9.2"
  }
}
